  "version": "1.0.2",
  "description": "AT&T Developer Hosted Server Library",
  "config": {
    "api_endpoint": "https://api.att.com",
    "authorize_uri": "/oauth/v4/authorize",
    "info": {
      "dhs_platform": "node",
      "token_uri": "/oauth/v4/token",
      "e911id_uri": "/emergencyServices/v1/e911Locations",
      "ewebrtc_uri": "/RTC/v1",
      "scope_map": {
        "MOBILE_NUMBER": "WEBRTCMOBILE",
        "VIRTUAL_NUMBER": "WEBRTC",
        "ACCOUNT_ID": "WEBRTC",
        "E911": "EMERGENCYSERVICES"
      }
    }
  },
  "keywords": [
    "att",
    "at&t",
    "att developer",
    "at&t developer",
    "webrtc",
    "enhanced webrtc",
    "ewebrtc",
    "dhs",
    "developer hosted server"
