  "main": "js/att.dhs.js",
  "files": [
    "js",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "restify": "^2.8.5"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "grunt": "^0.4.5",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-jsdoc": "^0.5.8",
    "grunt-jslint": "^1.1.12",
    "grunt-mocha-cli": "^1.11.0",
    "mocha": "^1.21.4",
    "mocha-sinon": "^1.1.4",
